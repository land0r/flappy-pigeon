{
  "name": "flappy-pigeon",
  "version": "1.0.0",
  "description": "A simple Flappy Bird clone featuring a pigeon",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "python3 -m http.server 8000",
    "dev": "concurrently \"npm run watch\" \"live-server --port=8000 --watch=dist,index.html --wait=200\"",
    "clean": "rm -rf dist",
    "start": "npm run build && npm run serve"
  },
  "keywords": [
    "game",
    "html5",
    "canvas",
    "typescript",
    "flappy-bird"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "live-server": "^1.2.2",
    "typescript": "^5.9.2"
  }
}
